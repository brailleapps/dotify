            <field>
               <string value=""/>
            </field>
            <field>
               <string value="⠤⠤⠤⠤⠤⠤⠤⠤"/>
            </field>
            <field>
               <string value="⠿⠿⠿⠿"/>
            </field>
         </footer>
      </default-template>
   </layout-master>
   <sequence master="a">
      <block>⠤⠤⠤
            </block>
   </sequence>
</obfl>