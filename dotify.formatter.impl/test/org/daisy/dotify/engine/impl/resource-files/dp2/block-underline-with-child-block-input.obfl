         <header/>
         <footer/>
      </default-template>
   </layout-master>
   <sequence master="main">
      <block underline-style="solid">⠌⠌⠌⠌
              <block>⠌⠌⠌⠌
              </block>
      </block>
   </sequence>
</obfl>