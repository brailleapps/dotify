<?xml version="1.0" encoding="UTF-8"?>
<obfl xmlns="http://www.daisy.org/ns/2011/obfl" version="2011-1" xml:lang="und" hyphenate="false">
   <meta>
      <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">row-spacing-nested-blocks</dc:title>
      <dc:description xmlns:dc="http://purl.org/dc/elements/1.1/">Tests row-spacing on block nested in block with border bottom and default row-spacing.</dc:description>
   </meta>
   <layout-master name="a" page-width="40" page-height="25" duplex="true" page-number-variable="page">
      <default-template>
         <header/>
         <footer/>
      </default-template>
   </layout-master>
   <sequence master="a">
      <block border-bottom-style="solid" border-bottom-align="center"><block row-spacing="2.0">⠿⠿⠿⠿⠿⠿⠿⠿ </block>
      </block>
      <block>⠿⠿⠿⠿⠿⠿⠿⠿ </block>
   </sequence>
</obfl>