<?xml version="1.0" encoding="UTF-8"?>
<obfl xmlns="http://www.daisy.org/ns/2011/obfl" version="2011-1" xml:lang="und" hyphenate="false">
   <meta>
      <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">table-leaders</dc:title>
      <dc:description xmlns:dc="http://purl.org/dc/elements/1.1/">Tests leaders within table cells.</dc:description>
   </meta>
   <layout-master name="main" page-width="20" page-height="5" duplex="true" page-number-variable="page">
      <default-template>
         <header/>
         <footer/>
      </default-template>
   </layout-master>
   <sequence master="main">
      <table table-col-spacing="1" preferred-empty-space="0">
         <tr>
            <td>⠿⠿⠿⠿⠿</td>
            <td>⠿⠿⠿⠿⠿</td>
            <td>⠿⠿⠿⠿⠿</td>
         </tr>
         <tr>
            <td>⠿⠿ <leader pattern="⠄" position="100%" align="right"/></td>
            <td>⠿⠿⠿ <leader pattern="⠄" position="100%" align="right"/></td>
            <td>⠿⠿⠿</td>
         </tr>
         <tr>
            <td>⠿ <leader pattern="⠄" position="100%" align="right"/></td>
            <td>⠿ <leader pattern="⠄" position="100%" align="right"/></td>
            <td>⠿</td>
         </tr>
      </table>
   </sequence>
</obfl>