            <td>⠿⠿⠿⠿</td>
            <td col-span="2">⠿⠿⠿⠿⠿⠿⠿⠿⠿</td>
            <td>⠿⠿⠿⠿</td>
         </tr>
         <tr>
            <td>⠿⠿</td>
            <td>⠿⠿</td>
            <td>⠿⠿</td>
            <td>⠿⠿</td>
         </tr>
         <tr>
            <td>⠿⠿</td>
            <td>⠿⠿</td>
            <td>⠿⠿</td>
            <td>⠿⠿</td>
         </tr>
      </table>
   </sequence>
</obfl>